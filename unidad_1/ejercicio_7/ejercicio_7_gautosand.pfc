program ejercicio_4;
const 
  N = 10; NP  = 2; LNG = 19;

type  myArray = array[0..LNG] of integer;

var 
  array1 : myArray;

process type cargar_vector(rango : integer; var sem: semaphore);
var 
  j, posInicial: integer;
begin
  posInicial := rango*N;
  
  for j := posInicial to posInicial+N-1 do
  begin
    wait(sem);
    writeln('soy ', rango, 'cargando ', j);
    array1[j] := rango;
    signal(sem);
  end;
  
  

end;

var
  i:integer;
  p0, p1 : cargar_vector;
  s: semaphore;
begin
  initial(s, 1);
  cobegin
    p0(0, s);
    p1(1, s);      
  coend;

  for i := 0 to LNG do
    write(array1[i], ' - ');
end.
